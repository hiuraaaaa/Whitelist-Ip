{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/ips/check",
      "dest": "/api/ips/check.js"
    },
    {
      "src": "/ips/add",
      "dest": "/api/ips/add.js"
    },
    {
      "src": "/ips/remove",
      "dest": "/api/ips/remove.js"
    },
    {
      "src": "/ips/list",
      "dest": "/api/ips/list.js"
    },
    {
      "src": "/ips/toggle-all",
      "dest": "/api/ips/toggle-all.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
